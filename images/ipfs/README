docker manifest create watorvapor/js-ipfs watorvapor/js-ipfs-arm64v8 watorvapor/js-ipfs-amd64
docker manifest annotate watorvapor/js-ipfs watorvapor/js-ipfs-arm64v8 --os linux --arch arm64
docker manifest annotate watorvapor/js-ipfs watorvapor/js-ipfs-amd64 --os linux --arch amd64
docker manifest push watorvapor/js-ipfs
